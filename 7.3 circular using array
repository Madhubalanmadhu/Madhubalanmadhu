#include<stdio.h>
#include<stdlib.h>
#define size 6
int queue[size];
int front=-1,rear=-1;
int checkFull ()
{
  if ((front==rear+1)||(front==0&&rear==size-1))
    {
      return 1;
    }
  return 0;
}
int isEmpty ()
{
  if (front==-1)
    {
      return 1;
    }
  return 0;
}
void enqueue(int value)
{
  if (checkFull())
    printf ("Overflow\n");

  else
    {
      if (front==-1)
	front=0;

      rear=(rear+1)%size;
      queue[rear]=value;
      printf("%d was enqueued to circular queue\n",value);
    }
}
int dequeue ()
{
  int variable;
  if (isEmpty ())
    {
      printf ("Underflow\n");
      return -1;
    }
  else
    {
      variable=queue[front];
      if (front==rear)
	{
	  front=rear=-1;
	}
      else
	{
	  front=(front+1)%size;
	}
      printf("%d was dequeued from circular queue\n",variable);
      return 1;
    }
}
void display()
{
  int i;
  if (isEmpty ())
    printf ("Nothing to dequeue\n");
  else
    {
      printf ("\nThe queue looks like\n");
      for (i=front;i!=rear;i=(i+1)%size)
	{
	  printf ("%d ",queue[i]);
	}
      printf ("%d\n\n",queue[i]);

    }
}
int main ()
{
	printf("\n192321142");
	printf("\nmadhubalan\n");
  dequeue ();
  enqueue (15);
  enqueue (20);
  enqueue (25);
  enqueue (30);
  enqueue (35);
  display();
  dequeue ();
}
