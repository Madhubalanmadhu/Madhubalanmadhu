#include<stdio.h>
int j=0;
int main()
{
	int a[100],b[100],c[100];
	printf("madhubalan\n192321142\nEnter input:\n");
	printf("Enter 5 element in Array A:\n");
	for(int i=0;i<5;i++){
	scanf("%d",&a[i]);
	}
	printf("Enter 5 element in Array B:\n");
	for(int i=0;i<5;i++){
	scanf("%d",&b[i]);
	}
	//printf("Merged Array is:\n");
	for(int i=0;i<5;i++){
	c[i]=a[i];
	}
	for(int i=5;i<10;i++){
		
	c[i]=b[j];
	j++;
	}
	j+=1;
	printf("Merged array is:\n"); 
	for(int i=0;i<10;i++){
	printf("%dÂ ",c[i]);
	}
	
}
