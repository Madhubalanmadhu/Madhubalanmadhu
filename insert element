#include<stdio.h>
int main()
{
	printf("madhubalan\n192321142\n");
	int a[100],n,pos,ele;
	printf("Enter array Size\n");
	scanf("%d",&n);
	printf("Enter array element\n");
	for(int i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("Enter which position wish insert element\n");
	scanf("%d",&pos);
	printf("Enter which position wish insert element\n");
	scanf("%d",&ele);
	for(int i=n;i>=pos;i--)
	{
		a[i+1]=a[i];
	}
	a[pos]=ele;
	n++;
	for(int i=0;i<n;i++)
	{
		printf("%dÂ ",a[i]);
	}
}
