#include<stdio.h>
int main()
{
	printf("madhubalan\n192321142\n");
	int a[100],n,ele,pos;
	printf("Enter array Size\n");
	scanf("%d",&n);
	printf("Enter array element\n");
	for(int i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("Element which are repeated twice in the given are:\n");
	for(int i=0;i<n;i++)
	{
		for(int j=i+1;j<n;j++)
		{
			if(a[i]==a[j])
			{
				printf("%d ",a[i]);0
				break;
			}	
		}
	}	
}
